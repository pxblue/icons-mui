{
  "name": "@pxblue/icons-mui",
  "version": "1.0.12",
  "description": "PX Blue icons for Material UI",
  "main": "./icons/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pxblue/icons-mui.git"
  },
  "author": "px-blue <pxblue@eaton.com>",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1"
  },
  "scripts": {
    "test": "./buildTest.sh",
    "index": "cp node_modules/@pxblue/icons-svg/index.json ./index.json",
    "babelfy": "babel icons --presets=@babel/preset-react,@babel/preset-env --out-dir icons",
    "refresh": "yarn upgrade @pxblue/icons-svg",
    "clear": "rm -rf icons",
    "remove:sprite": "rm -rf node_modules/@pxblue/icons-svg/icons.svg",
    "mui": "yarn remove:sprite && cd mui-icon-builder && babel-node --config-file ./babel.config.js builder.js --output-dir ../icons --svg-dir ../node_modules/@pxblue/icons-svg && cd .. && yarn babelfy && yarn index",
    "publish:full": "yarn clear && yarn refresh && yarn mui && npm version patch && npm publish"
  },
  "bugs": {
    "url": "https://github.com/pxblue/icons-mui/issues"
  },
  "homepage": "https://github.com/pxblue/icons-mui#readme",
  "devDependencies": {
    "@svgr/cli":"^4.1.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@pxblue/icons-svg": "^1.0.0"
  },
  "keywords": [
    "icons",
    "material",
    "material-ui",
    "pxblue"
  ]
}
